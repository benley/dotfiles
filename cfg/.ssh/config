ForwardAgent no
HashKnownHosts no
ForwardX11 no
ForwardX11Trusted no

# Use gpg-agent by default, despite whatever gnome sets SSH_AUTH_SOCK to
IdentityAgent /run/user/%i/gnupg/S.gpg-agent.ssh

# Host aram.xkcd.com zoiks.net *.zoiks.net
#   Compression yes

Host zoiks.net osric.zoiks.net
  HostKeyAlias osric.zoiks.net

Host github.com
  Compression yes

