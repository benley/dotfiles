# ControlPersist doesn't work with OpenSSH_4.5p1 from debian Squeeze (stable)
# It *does* work with: Ubuntu 12.04, Ubuntu 12.10, MacOS 10.8, [...]

Host aram.xkcd.com
ForwardAgent yes
Compression yes

Host github.com
Compression yes
ForwardAgent no
ForwardX11 no
ForwardX11Trusted no

Host maya maya.zoiks.net
ForwardX11 no
ForwardX11Trusted no
Hostname maya.zoiks.net
#Port 2222
Compression yes
ControlMaster auto
#ControlPersist yes
ControlPath /tmp/.control_%r@%h:%p.%l

Host nyanbox.z nb.z
ForwardX11 yes
ForwardX11Trusted yes
Compression yes
ForwardAgent yes
ProxyCommand ssh -q sf.zoiks.net nc -q0 nb 22
#HostName sf.zoiks.net
#Port 2224
#ControlMaster auto
##ControlPersist yes
#ControlPath /tmp/.control_%r@%h:%p.%l

Host zoiks.net *.zoiks.net
GSSAPIAuthentication no
GSSAPIKeyExchange no
#PreferredAuthentications publickey,password
ForwardAgent yes
Compression yes

# VMs
Host vm-ubudev1.local vm1 vm-ubudev1
HostName 192.168.56.2
ForwardAgent yes
Compression no
ForwardX11 yes
ForwardX11Trusted yes
ControlPersist yes
ControlMaster auto
ControlPath /tmp/.ssh_%r@%h:%p

# CS
Host *.jenkins.cloudscaling.com
Port 51225
User ben@cloudscaling.com
ProxyCommand ssh -q -l "ben@cloudscaling.com" -p 51225 jenkins.cloudscaling.com diagnose-tunnel -suffix .jenkins.cloudscaling.com %h
UserKnownHostsFile ~/.ssh/jenkins_known_hosts
HashKnownHosts no
StrictHostKeyChecking no

Host pd pd.cloudscaling.com
HostName pd.cloudscaling.com
User ben


Host admin.lab admin.lab.cloudscaling.com
HostName admin.lab.cloudscaling.com
User ben
ForwardAgent yes
ForwardX11 no
ForwardX11Trusted no
Compression yes
ControlPersist 180
ControlMaster auto

Host lab*
User ben
ForwardAgent yes

Host zm.sv zm.sv.cloudscaling.com
HostName zm.sv.cloudscaling.com
User ben
ForwardAgent yes
ForwardX11 no
ForwardX11Trusted no
Compression yes
ControlMaster auto
ControlPersist 180
ControlPath /tmp/.ssh_%r@%h:%p

Host *.cloudscaling.com
User ben

Host 172.31.0.* 192.168.129.*
StrictHostKeyChecking no
ForwardAgent yes
ForwardX11 no
ForwardX11Trusted no
Compression no
UserKnownHostsFile ~/sheep/tmp/sheep_known_hosts
HashKnownHosts no

Host *.*
GSSAPIAuthentication no
GSSAPIDelegateCredentials no
GSSAPIKeyExchange no
GSSAPITrustDNS no
ForwardAgent no
Compression yes
ForwardX11 no
ForwardX11Trusted no
